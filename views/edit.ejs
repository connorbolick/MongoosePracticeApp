<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>new name form</title>
  </head>
  <body>
    <p>edit</p>
    <% for(i in users){ %>
      <form action="/owls/<%= users[i]._id %>" method="post">
        <input type="text" name="name" value="<%= users[i].name%>">
        <input type="hidden" name="id" value="<%= users[i]._id %>">
        <input type="submit" name="submit" value="Edit">
      </form>
      <p><%= users[i]  %></p>
    <% } %>
  </body>
</html>
